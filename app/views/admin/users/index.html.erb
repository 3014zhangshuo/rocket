<h1>用户管理面板</h1>
<table class="table table-bordered">
    <thead>
      <tr>
        <td>#</td>
        <td>用户名</td>
        <td>姓名</td>
        <td>创建时间</td>
        <td>管理</td>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
    <tr>
      <td> <%= user.id %></td>
      <td> <%= user.email %></td>
      <td> <%= user.user_name %></td>
      <td><%= user.created_at %></td>
      <td><%= link_to("Edit", edit_admin_user_path(user), :class => "btn btn-xs btn-info") %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
